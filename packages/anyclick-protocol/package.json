{
  "name": "@ewjdev/anyclick-protocol",
  "version": "0.2.0",
  "private": false,
  "description": "Canonical protocol specification for Anyclick interaction events",
  "keywords": [
    "anyclick",
    "protocol",
    "typescript",
    "types",
    "intent",
    "interaction-events"
  ],
  "license": "MIT",
  "author": "ewjdev",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewjdev/anyclick.git",
    "directory": "packages/anyclick-protocol"
  },
  "homepage": "https://anyclick.ewj.dev/docs/protocol",
  "bugs": {
    "url": "https://github.com/ewjdev/anyclick/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --format esm,cjs --dts --sourcemap --watch"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
