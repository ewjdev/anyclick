{
  "name": "@ewjdev/anyclick-jira",
  "version": "1.0.0",
  "private": false,
  "description": "Jira adapter for Anyclick - submit UI feedback directly to Jira issues",
  "keywords": [
    "feedback",
    "ui",
    "jira",
    "issues",
    "typescript"
  ],
  "license": "MIT",
  "author": "ewjdev",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewjdev/anyclick.git",
    "directory": "packages/anyclick-jira"
  },
  "homepage": "https://anyclick.ewj.dev/docs/adapters",
  "bugs": {
    "url": "https://github.com/ewjdev/anyclick/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap --watch"
  },
  "dependencies": {
    "@ewjdev/anyclick-core": "^1.4.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}

