{
  "name": "@anyclick/github",
  "version": "0.1.0",
  "private": false,
  "description": "GitHub adapter for UI feedback - includes browser HTTP adapter and Node-side GitHub integration",
  "keywords": [
    "feedback",
    "ui",
    "github",
    "issues",
    "typescript"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap --watch"
  },
  "dependencies": {
    "@anyclick/core": "*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
