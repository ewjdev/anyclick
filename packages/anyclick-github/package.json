{
  "name": "@ewjdev/anyclick-github",
  "version": "0.1.0",
  "private": false,
  "description": "GitHub adapter for UI feedback - includes browser HTTP adapter and Node-side GitHub integration",
  "keywords": [
    "feedback",
    "ui",
    "github",
    "issues",
    "typescript"
  ],
  "license": "MIT",
  "author": "ewjdev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewjdev/anyclick.git",
    "directory": "packages/anyclick-github"
  },
  "homepage": "https://anyclick.ewjdev.com/docs/adapters",
  "bugs": {
    "url": "https://github.com/ewjdev/anyclick/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ewjdev-anyclick-github": "./bin/setup-media-branch.js",
    "setup-media-branch": "./bin/setup-media-branch.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "predev": "yarn setup",
    "dev": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap --watch",
    "setup": "node bin/setup-media-branch.js"
  },
  "dependencies": {
    "@ewjdev/anyclick-core": "*"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
