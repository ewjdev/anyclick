{
  "name": "@ewjdev/anyclick-react",
  "version": "3.0.0",
  "private": false,
  "description": "React provider and context menu UI for UI feedback capture",
  "keywords": [
    "feedback",
    "ui",
    "context-menu",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "author": "ewjdev",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewjdev/anyclick.git",
    "directory": "packages/anyclick-react"
  },
  "homepage": "https://anyclick.ewj.dev/docs/react",
  "bugs": {
    "url": "https://github.com/ewjdev/anyclick/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./presets": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:vite": "vite",
    "build:vite": "vite build",
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --external react",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --format esm,cjs --dts --sourcemap --external react --watch"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.114",
    "@ewjdev/anyclick-core": "^1.4.0",
    "@ewjdev/anyclick-pointer": "^3.0.0",
    "lucide-react": "^0.544.0",
    "zustand": "^5.0.2",
    "tailwindcss": "4.1.17"
  },
  "peerDependencies": {
    "react": ">=19.2.1",
    "react-dom": ">=19.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.32",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "rimraf": "^6.0.1",
    "vite": "^5.2.0",
    "@tailwindcss/vite": "4.1.17"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
