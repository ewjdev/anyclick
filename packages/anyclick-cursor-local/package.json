{
  "name": "@ewjdev/anyclick-cursor-local",
  "version": "0.1.0",
  "private": false,
  "description": "Local Cursor CLI adapter for UI feedback - run cursor-agent locally during development",
  "keywords": [
    "feedback",
    "ui",
    "cursor",
    "cli",
    "cursor-agent",
    "development",
    "typescript"
  ],
  "license": "MIT",
  "author": "ewjdev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewjdev/anyclick.git",
    "directory": "packages/anyclick-cursor-local"
  },
  "homepage": "https://anyclick.ewjdev.com/docs/adapters",
  "bugs": {
    "url": "https://github.com/ewjdev/anyclick/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "uifeedback-local-server": "./dist/server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts src/server.ts --format esm,cjs --dts --sourcemap --watch",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@ewjdev/anyclick-core": "*"
  },
  "devDependencies": {
    "@types/node": "^20.17.32",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
